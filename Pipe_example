 %%% dT/dt = d^2T/dx^2 - Partial derivatives
 %%%% T = temperature = f(x,t)
 %%% time derivative = second spatial derviative
 %%% ( T(x, t+dt) - T(x,t) )  / dt = k*( T(x+dx, t) - 2*T(x,t) + T(x-1,t) ) / dx^2
