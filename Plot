
import matplotlib.pyplot as plt


a = [63233, 4333, 271, 18, 2]

I = [0,1,2,3,4]               

b = [0.931856, 0.063854, 0.003993, 0.000265, 0.000029]

plt.plot(I, b, linewidth=2,linestyle='dashed',color='g', label = 'Data Csv')
plt.xlabel('Number of Claims')
plt.ylabel('Frequnecy as a percentage')
plt.title("Number claims of a year")
plt.axis([-1, 5, 0, 1])
plt.legend(loc='upper left', fontsize=10.5)
plt.savefig("Intail_tests.png")


